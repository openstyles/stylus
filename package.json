    "@automattic/webpack-inline-constant-exports-plugin": "^1.0.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\" --cache",
    "test": "npm run lint && npm run test-csslint",
    "test-csslint": "node --import=extensionless/register tools/test-css.js",
    "update-locales": "tx pull --all && node tools/fix-transifex.js && git commit -m \"update locales\" _locales",
    "update-transifex": "tx push -s",
    "build-icons": "node tools/build-icons",
    "build-chrome": "webpack-cli --define-process-env-node-env chrome",
    "build-firefox": "webpack-cli --define-process-env-node-env firefox",
    "build-mv3": "webpack-cli --define-process-env-node-env chrome-mv3",
    "watch": "webpack-cli watch --define-process-env-node-env DEV",
    "watch-mv3": "webpack-cli watch --define-process-env-node-env chrome-mv3",
    "zip": "npm test && node tools/build-zip.js",
    "postinstall": "node tools/postinstall.js",
    "preversion": "npm test",
    "version": "sync-version manifest.json && git add .",
    "postversion": "npm run zip && git push --follow-tags"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}
